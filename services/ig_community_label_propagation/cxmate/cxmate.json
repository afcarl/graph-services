{
  "general": {
    "location": "0.0.0.0:80",
    "logger": {
      "debug": true
    }
  },
  "service": {
    "location": "ig-community_label_propagation-service:8080",
    "title": "igraph service",
    "version": "1.0.0",
    "author": "",
    "email": "",
    "website": "",
    "repository": "https://github.com/idekerlab/graph-services",
    "description": "Simple service applying basic graph layout algorithm",
    "license": "MIT",
    "language": "Python",
    "parameters": [{
        "name": "stat-name",
        "default": "degree",
        "description": "Target algorithm name"
    }],
    "input": [{
      "label": "in_net",
      "description": "In net",
      "aspects": ["nodes", "edges", "nodeAttributes", "edgeAttributes"]
    }],

    "singletonInput": true,

    "output": [{
      "label": "out_net",
      "description": "Network attributes",
      "aspects": ["nodes", "edges", "nodeAttributes", "edgeAttributes", "networkAttributes"]
    }],
    "singletonOutput": true
  }
}
