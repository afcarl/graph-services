{
  "general": {
    "location": "0.0.0.0:80",
    "logger": {
      "debug": true
    }
  },
  "service": {
    "location": "gt-layout-service:8080",
    "title": "graph-tool service",
    "version": "1.0.0",
    "author": "Keiichiro Ono",
    "email": "kono@ucsd.edu",
    "website": "",
    "repository": "https://github.com/idekerlab/graph-services",
    "description": "Simple service applying basic graph layout algorithm",
    "license": "MIT",
    "language": "Python",
    "parameters": [{
        "name": "layout-name",
        "default": "sfdp",
        "description": "Layout algorithm name"
    }],
    "input": [{
      "label": "in_net",
      "description": "In net",
      "aspects": ["nodes", "edges"]
    }],

    "singletonInput": true,

    "output": [{
      "label": "out_net",
      "description": "Layout data",
      "aspects": ["nodes", "edges", "cartesianLayout"]
    }],
    "singletonOutput": true
  }
}
